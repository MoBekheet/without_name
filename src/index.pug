include assets/includes/_variables
include assets/includes/_mixins
include assets/includes/mixins/_card
doctype html
html(lang="en")
  //- HEAD
  head
    include assets/includes/_head
    link(rel="stylesheet", href="assets/css/home.min.css")
  body
    .loading.d-flex.align-items-center.justify-content-center
      .sk-folding-cube
        div(class="sk-cube1 sk-cube")
        div(class="sk-cube2 sk-cube")
        div(class="sk-cube4 sk-cube")
        div(class="sk-cube3 sk-cube")
    a(href="#" id="btnSubmitYourSite_absolute" class="fi flaticon-add p-3 link" data-hover="!أعرض موقعك الأن")
    include assets/includes/mixins/_navbar&aside
    include assets/includes/mixins/_login
    include assets/includes/mixins/_cursor
    
    main
      div(data-scroll)
        section#site_of_the_day.py-5
          #split
          a(href="#" class="overlay link" data-hover="عرض")
          .dots
          #items.m-0.d-flex.align-items-center.justify-content-start
            .item.item_scroll.link: .mask_img.item__img-wrap: img(class="item__img" src="assets/images/1.jpg", alt=null)
            .item.item_scroll.link: .mask_img.item__img-wrap: img(class="item__img" src="assets/images/2.jpg", alt=null)
            .item.item_scroll.link: .mask_img.item__img-wrap: img(class="item__img" src="assets/images/3.jpg", alt=null)
            .item.item_scroll.link: .mask_img.item__img-wrap: img(class="item__img" src="assets/images/4.jpg", alt=null)
          #caption.d-flex.align-items-center.flex-column
            #info.mb-4
              #heading-large
                h1: a(href="#" class="link") موقع اخباري
              #data: span بواسطة 
                a(href="#" class="link name") محمود بخيت 
                span من 
                  a(href="#" class="link"): strong مصر 
                  span تقييم 
                    span.rating 7
            
            .box_noteSite
              ul.d-flex.flex-wrap.justify-content-around.justify-content-center
                li(class="dislink design", data-hover="التصميم")
                li(class="dislink usability", data-hover="سهولة الاستخدام")
                li(class="dislink creativity", data-hover="الإبداع")
                li(class="dislink content", data-hover="المحتوى")
                li(class="dislink responsive", data-hover="متجاوب")
            #btnSubmitYourSite
              a(href="#" class="px-4 py-3 link") !أعرض موقعك الأن
          .followers.position.d-none.d-lg-block
            .container
              .row.d-flex.justify-content-end
                ul.p-0.m-0.d-flex
                  li.more_users.mx-1: a(href="#" class="link d-flex align-items-center justify-content-center")
                    i.flaticon-user.fi
                    span.num 23
                  - for (let x = 0; x < 5; x++)
                      li.mx-1.user_overflow: a(href="#" class="link d-block"): img(src="assets/images/users/2.jpg", alt="")
          
          #info_down.d-flex.justify-content-center
            button(type="button" id="close" class="link mt-4" data-hover="خروج"): i.fi.flaticon-cancel
        
        section#Winners.my-5.py-5
          .dots
          .container
            .head_title.mb-5.mt-0
              h2 مواقع اليوم 
                span الفائزين السابقين
          div(class="swiper__slider swiper")
            div(class="swiper__inner")
            div(class="proxy link move")
              div(class="swiper__content d-flex")
                div(class="swiper-slide slide")
                  +swiper("assets/images/4.jpg",null,"assets/images/users/1.jpg",null,"محمود بخيت","30","مصر","3 فبراير 2018")
